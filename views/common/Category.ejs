<div class="content-wrapper card">
  <div class="card-header">
    <h3 class="card-title">Category List</h3>
  </div>
  <!-- /.card-header -->
  <div class="card-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>items</th>
          <th>Update</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        <%if(typeof(list)!="undefined"){%>
          <%list.map((li)=>{%>

            <tr>
              <td class="border-0"><%-li.categoryName%></td>
              <td class="border-0"><%-li.children.length%></td>
              <td> <button type="button" class="btn btn-block btn-outline-warning btn-sm" style="width: 40%;">Update</button></td>
              <td> <button type="button" class="btn btn-block btn-outline-danger btn-sm" style="width: 40%;">Delete</button></td>
            </tr>
            <%})%>
          
          <%}%>
      </tbody>
      <tfoot>
        <tr>
          <th>Name</th>
          <th>Items</th>
          <th>Update</th>
          <th>Delete</th>
        </tr>
      </tfoot>
    </table>
  </div>
  <!-- /.card-body -->
</div>